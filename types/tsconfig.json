{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../",
    "paths": {
      "bree": ["."]
    },
    "moduleResolution": "node"
  },
  "include": ["src"]
}
